<?xml version="1.0" encoding="utf-8"?>
<widget id="com.wisape.android" version="1.0.0">
    <name>Wisape</name>

    <access origin="*"/>

    <preference name="loglevel" value="DEBUG" />
    <preference name="Orientation" value="portrait"/>
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />

    <feature name="PhotoSelector">
        <param name="android-package" value="com.wisape.android.cordova.PhotoSelectorPlugin"/>
    </feature>
</widget>